  "repository": {
    "type": "git",
    "url": ""
  },
  "browserslist": [
    "> 1% and last 2 versions",
    "Firefox ESR",
    "ios_saf 12",
    "safari 12",
    "ie 11",
    "not ie_mob 11",
    "not android 4.4.3-4.4.4",
    "not OperaMini all"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/static_compiled/",
      "/venv/"
    ],
    "collectCoverageFrom": [
      "**/tna_account_management/static_src/javascript/**/*.js"
    ]
  },
  "scripts": {
    "//[ Linting & formatting ]//": "",
    "lint:css": "stylelint --report-needless-disables './tna_account_management/static_src/sass' './tna_account_management/static_src/javascript/vue_components'",
    "lint:js": "eslint --report-unused-disable-directives .",
    "lint:format": "prettier --check '**/?(.)*.{md,css,scss,js,json,yaml,yml,vue}'",
    "lint": "npm run lint:format && npm run lint:js && npm run lint:css",
    "format": "prettier --write '**/?(.)*.{md,css,scss,js,json,yaml,yml,vue}'",
    "//[ Tests ]//": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "//[ Build tasks ]//": "",
    "build": "webpack --mode development --progress",
    "build:prod": "webpack --mode production",
    "start": "webpack --mode development --progress --watch",
    "start:reload": "webpack-dev-server"
  },
